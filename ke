local dragging = false
local dragInput
local dragStart
local startPos

local function updateDrag(input)
    local delta = input.Position - dragStart
    MainFrame.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
end

Header.InputBegan:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 then
        dragging = true
        dragStart = input.Position
        startPos = MainFrame.Position
        
        input.Changed:Connect(function()
            if input.UserInputState == Enum.UserInputState.End then
                dragging = false
            end
        end)
    end
end)

Header.InputChanged:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseMovement then
        dragInput = input
    end
end)

UserInputService.InputChanged:Connect(function(input)
    if input == dragInput and dragging then
        updateDrag(input)
    end
end)

local function applyButtonHoverEffect(button, defaultColor, hoverColor)
    button.MouseEnter:Connect(function()
        TweenService:Create(button, TweenInfo.new(0.2), {BackgroundColor3 = hoverColor}):Play()
    end)
    
    button.MouseLeave:Connect(function()
        TweenService:Create(button, TweenInfo.new(0.2), {BackgroundColor3 = defaultColor}):Play()
    end)
end

applyButtonHoverEffect(ValidateButton, Color3.fromRGB(85, 170, 255), Color3.fromRGB(65, 140, 220))
applyButtonHoverEffect(LinkvertiseButton, Color3.fromRGB(40, 40, 40), Color3.fromRGB(50, 50, 50))
applyButtonHoverEffect(LootlabsButton, Color3.fromRGB(40, 40, 40), Color3.fromRGB(50, 50, 50))
applyButtonHoverEffect(DiscordButton, Color3.fromRGB(40, 40, 40), Color3.fromRGB(50, 50, 50))
applyButtonHoverEffect(CloseButton, Color3.fromRGB(40, 40, 40), Color3.fromRGB(255, 85, 85))
applyButtonHoverEffect(MinimizeButton, Color3.fromRGB(40, 40, 40), Color3.fromRGB(60, 60, 60))

KeyTextbox.Focused:Connect(function()
    TweenService:Create(KeyTextboxHolder, TweenInfo.new(0.3), {BackgroundColor3 = Color3.fromRGB(30, 30, 30)}):Play()
    TweenService:Create(KeyTextboxStroke, TweenInfo.new(0.3), {Color = Color3.fromRGB(85, 170, 255)}):Play()
end)

KeyTextbox.FocusLost:Connect(function()
    TweenService:Create(KeyTextboxHolder, TweenInfo.new(0.3), {BackgroundColor3 = Color3.fromRGB(25, 25, 25)}):Play()
    TweenService:Create(KeyTextboxStroke, TweenInfo.new(0.3), {Color = Color3.fromRGB(60, 60, 60)}):Play()
end)

ValidateButton.MouseButton1Click:Connect(function()
    local keyText = KeyTextbox.Text
    
    if keyText == "" then
        StatusLabel.Text = "enter a key"
        StatusLabel.TextColor3 = Color3.fromRGB(255, 85, 85)
        createNotification("Error", "Enter your key to continue", 3, Color3.fromRGB(255, 85, 85), ScreenGui)
        return
    end
    
    StatusLabel.Text = "Checking key..."
    StatusLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
    
    task.wait(1)
    
    local valid, status = validateKey(keyText)
    
    if not valid then
        StatusLabel.Text = "Invalid key"
        StatusLabel.TextColor3 = Color3.fromRGB(255, 85, 85)
        createNotification("Invalid Key", "The key you entered is not valid", 3, Color3.fromRGB(255, 85, 85), ScreenGui)
        return
    end
    
    if status == "KEY_VALID" then
        StatusLabel.Text = "Loading SnareMvsd..."
        StatusLabel.TextColor3 = Color3.fromRGB(85, 255, 127)
        
        createNotification("Success", "Loading SnareMvsd...", 3, Color3.fromRGB(85, 255, 127), ScreenGui)
        
        local execSuccess, execError = executeScript(keyText)
        if not execSuccess then
            createNotification("Script Error", execError, 5, Color3.fromRGB(255, 85, 85), ScreenGui)
            return
        end
        
        task.delay(2, function()
            TweenService:Create(MainFrame, TweenInfo.new(0.5, Enum.EasingStyle.Quint), {Size = UDim2.fromOffset(0, 460)}):Play()
            task.wait(0.5)
            ScreenGui:Destroy()
        end)
    else
        StatusLabel.Text = "Checking key failed"
        StatusLabel.TextColor3 = Color3.fromRGB(255, 85, 85)
        createNotification("Error", "Key failed", 3, Color3.fromRGB(255, 85, 85), ScreenGui)
    end
end)

LinkvertiseButton.MouseButton1Click:Connect(function()
    setclipboard("https://linkvertise.com/1400986/OyIukjS6OJhP?o=sharing")
    createNotification("Link Copied", "Linkvertise URL copied to clipboard", 3, Color3.fromRGB(85, 170, 255), ScreenGui)
end)

