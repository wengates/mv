local dragging = false
local dragInput
local dragStart
local startPos

local function updateDrag(input)
    local delta = input.Position - dragStart
    MainFrame.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
end

Header.InputBegan:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 then
        dragging = true
        dragStart = input.Position
        startPos = MainFrame.Position
        
        input.Changed:Connect(function()
            if input.UserInputState == Enum.UserInputState.End then
                dragging = false
            end
        end)
    end
end)

Header.InputChanged:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseMovement then
        dragInput = input
    end
end)

UserInputService.InputChanged:Connect(function(input)
    if input == dragInput and dragging then
        updateDrag(input)
    end
end)

local function applyButtonHoverEffect(button, defaultColor, hoverColor)
    button.MouseEnter:Connect(function()
        TweenService:Create(button, TweenInfo.new(0.2), {BackgroundColor3 = hoverColor}):Play()
    end)
    
    button.MouseLeave:Connect(function()
        TweenService:Create(button, TweenInfo.new(0.2), {BackgroundColor3 = defaultColor}):Play()
    end)
end

applyButtonHoverEffect(ValidateButton, Color3.fromRGB(85, 170, 255), Color3.fromRGB(65, 140, 220))
applyButtonHoverEffect(LinkvertiseButton, Color3.fromRGB(40, 40, 40), Color3.fromRGB(50, 50, 50))
applyButtonHoverEffect(LootlabsButton, Color3.fromRGB(40, 40, 40), Color3.fromRGB(50, 50, 50))
applyButtonHoverEffect(DiscordButton, Color3.fromRGB(40, 40, 40), Color3.fromRGB(50, 50, 50))
applyButtonHoverEffect(CloseButton, Color3.fromRGB(40, 40, 40), Color3.fromRGB(255, 85, 85))
applyButtonHoverEffect(MinimizeButton, Color3.fromRGB(40, 40, 40), Color3.fromRGB(60, 60, 60))

KeyTextbox.Focused:Connect(function()
    TweenService:Create(KeyTextboxHolder, TweenInfo.new(0.3), {BackgroundColor3 = Color3.fromRGB(30, 30, 30)}):Play()
    TweenService:Create(KeyTextboxStroke, TweenInfo.new(0.3), {Color = Color3.fromRGB(85, 170, 255)}):Play()
end)

KeyTextbox.FocusLost:Connect(function()
    TweenService:Create(KeyTextboxHolder, TweenInfo.new(0.3), {BackgroundColor3 = Color3.fromRGB(25, 25, 25)}):Play()
    TweenService:Create(KeyTextboxStroke, TweenInfo.new(0.3), {Color = Color3.fromRGB(60, 60, 60)}):Play()
end)

ValidateButton.MouseButton1Click:Connect(function()
    local keyText = KeyTextbox.Text
    
    if keyText == "" then
        StatusLabel.Text = "Enter a key"
        StatusLabel.TextColor3 = Color3.fromRGB(255, 85, 85)
        createNotification("Error", "Enter your key to continue", 3, Color3.fromRGB(255, 85, 85), ScreenGui)
        return
    end
    
    StatusLabel.Text = "Checking key..."
    StatusLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
    
    local valid, message = validateKey(keyText)
    
    if not valid then
        StatusLabel.Text = message
        StatusLabel.TextColor3 = Color3.fromRGB(255, 85, 85)
        createNotification("Invalid Key", message, 3, Color3.fromRGB(255, 85, 85), ScreenGui)
        return
    end
    
    StatusLabel.Text = "Loading SnareMvsd..."
    StatusLabel.TextColor3 = Color3.fromRGB(85, 255, 127)
    
    createNotification("Success", "Loading SnareMvsd...", 3, Color3.fromRGB(85, 255, 127), ScreenGui)
    
    local execSuccess, execError = executeScript(keyText)
    
    if not execSuccess then
        StatusLabel.Text = "Load failed"
        StatusLabel.TextColor3 = Color3.fromRGB(255, 85, 85)
        createNotification("Script Error", execError, 5, Color3.fromRGB(255, 85, 85), ScreenGui)
        return
    end
    
    task.delay(2, function()
        TweenService:Create(MainFrame, TweenInfo.new(0.5, Enum.EasingStyle.Quint), {Size = UDim2.fromOffset(0, 460)}):Play()
        task.wait(0.5)
        ScreenGui:Destroy()
    end)
end)

LinkvertiseButton.MouseButton1Click:Connect(function()
    setclipboard("https://linkvertise.com/1400986/OyIukjS6OJhP?o=sharing")
    createNotification("Link Copied", "Linkvertise URL copied to clipboard", 3, Color3.fromRGB(85, 170, 255), ScreenGui)
end)

LootlabsButton.MouseButton1Click:Connect(function()
    setclipboard("https://lootdest.org/s?NfPeo1rV")
    createNotification("Link Copied", "Lootlabs URL copied to clipboard!", 3, Color3.fromRGB(85, 170, 255), ScreenGui)
end)

DiscordButton.MouseButton1Click:Connect(function()
    setclipboard("https://discord.gg/snare")
    createNotification("Invite Copied", "Discord invite copied to clipboard!", 3, Color3.fromRGB(88, 101, 242), ScreenGui)
end)

CloseButton.MouseButton1Click:Connect(function()
    TweenService:Create(MainFrame, TweenInfo.new(0.3, Enum.EasingStyle.Back, Enum.EasingDirection.In), {Size = UDim2.fromOffset(0, 460)}):Play()
    task.wait(0.3)
    ScreenGui:Destroy()
end)

MinimizeButton.MouseButton1Click:Connect(function()
    MainFrame.Visible = false
    
    local RestoreButton = Instance.new("TextButton")
    RestoreButton.Name = "RestoreButton"
    RestoreButton.Text = "SnareMvsd"
    RestoreButton.TextColor3 = Color3.fromRGB(255, 255, 255)
    RestoreButton.TextSize = 12
    RestoreButton.FontFace = Font.new("rbxasset://fonts/families/GothamSSm.json", Enum.FontWeight.Bold)
    RestoreButton.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
    RestoreButton.BorderSizePixel = 0
    RestoreButton.Position = UDim2.new(0, 10, 0, 10)
    RestoreButton.Size = UDim2.fromOffset(100, 30)
    RestoreButton.Parent = ScreenGui
    
    local RestoreCorner = Instance.new("UICorner")
    RestoreCorner.CornerRadius = UDim.new(0, 8)
    RestoreCorner.Parent = RestoreButton
    
    local RestoreStroke = Instance.new("UIStroke")
    RestoreStroke.Color = Color3.fromRGB(60, 60, 60)
    RestoreStroke.Thickness = 1
    RestoreStroke.Parent = RestoreButton
    
    RestoreButton.MouseButton1Click:Connect(function()
        MainFrame.Visible = true
        RestoreButton:Destroy()
    end)
end)

task.delay(1, function()
    createNotification("Welcome", "Enter your key to access SnareMvsd", 5, Color3.fromRGB(85, 170, 255), ScreenGui)
end)
